<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.waldhart.zoecomm" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ZoeComm</name>
    <description>
        A simple to use communication tool for non-verbal persons
    </description>
    <author email="" href="http://github.com/juleswh">
        Jules Waldhart and Martin Conant
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="~1.3.2" />
    <engine name="android" spec="~5.0.0" />
    <engine name="browser" spec="~4.1.0" />
</widget>
